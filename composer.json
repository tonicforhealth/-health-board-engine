{
    "name": "tonicforhealth/health-board",
    "description": "Tonic health check board app",
    "require": {
        "symfony/console": "~3.0",
        "guzzlehttp/guzzle": "^5.3|^6.1",
        "elasticsearch/elasticsearch": "^2.1",
        "predis/predis": "^1.0",
        "stomp-php/stomp-php": "^4.0",
        "stage1/docker-php": "dev-guzzle5",
        "danielgsims/php-collections": "^1.4",
        "herzult/php-ssh": "^1.1",
        "supervisorphp/supervisor": "^3.0",
        "lstrojny/fxmlrpc": "^0.10.0",
        "egeloen/http-adapter": "^1.0",
        "pimple/pimple": "^3.0",
        "diversen/simple-php-translation": "^1.0",
        "twig/twig": "^1.23",
        "doctrine/orm": "^2.5",
        "php-http/guzzle5-adapter": "dev-master",
        "php-http/client-common": "~1.0",
        "php-http/plugins": "~1.0",
        "php-http/message": "~1.2",
        "swiftmailer/swiftmailer": "~5.4",
        "php-imap/php-imap": "~2.0",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "~3.0",
        "tonicforhealth/health-checker-check-email": "^0.1.5",
        "tonicforhealth/health-checker-check": "~0.1",
        "puli/composer-plugin": "dev-classmap as 1.0-beta10",
        "tonicforhealth/health-checker-incident-component": "^0.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "php-http/mock-client": "~0.1",
        "phpunit/dbunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TonicHealthCheck\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:printu/docker-php.git"
        },
        {
        "type": "vcs",
        "url": "https://github.com/thewilkybarkid/composer-plugin"
        }
    ],
    "authors": [
        {
            "name": "Dmitry Gopkalo",
            "email": "dmitry.gopkalo@tonicforhealth.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "github-oauth":{
            "github.com":"092339f98989268ae95aa344ac0d551063069494"
        }

    }
}
